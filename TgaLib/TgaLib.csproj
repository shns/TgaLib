<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFrameworks>net40;netcoreapp3.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <Version>1.0.3</Version>
    <Description>Library to decode TARGA image format.</Description>
    <Authors>shns</Authors>
    <Copyright>Copyright © 2016 shns</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageTags>tga targa</PackageTags>
    <PackageProjectUrl>https://github.com/shns/TgaLib</PackageProjectUrl>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>